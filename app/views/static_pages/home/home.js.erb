$('.col-lg-12.homepage-non-auth-user-sign-in-link').bind('ajax:complete', function() {
  $('.col-lg-12.homepage-non-auth-user-sign-up').replaceWith('<%= j render('static_pages/home/signin_form') %>');
  $('.col-lg-12.homepage-non-auth-user-sign-in-link').replaceWith('<%= j render('static_pages/home/signup_link') %>');
});

$('.col-lg-12.homepage-non-auth-user-sign-up-link').bind('ajax:complete', function() {
  $('.col-lg-12.homepage-non-auth-user-sign-in').replaceWith('<%= j render('static_pages/home/signup_form') %>');
  $('.col-lg-12.homepage-non-auth-user-sign-up-link').replaceWith('<%= j render('static_pages/home/signin_link')%>');
});
