$('.homepage-non-auth-user-sign-in-link').bind('ajax:complete', function() {
  $('.homepage-non-auth-user-sign-up').replaceWith('<%= j render('static_pages/home/sign_in_form') %>');
  $('.homepage-non-auth-user-sign-in-link').replaceWith('<%= j render('static_pages/home/sign_up_link') %>');
});

$('.homepage-non-auth-user-sign-up-link').bind('ajax:complete', function() {
  $('.homepage-non-auth-user-sign-in').replaceWith('<%= j render('static_pages/home/sign_up_form') %>');
  $('.homepage-non-auth-user-sign-up-link').replaceWith('<%= j render('static_pages/home/sign_in_link')%>');
});
